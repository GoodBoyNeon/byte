{
  "name": "byte",
  "version": "0.1.1",
  "description": "Byte is a unique general use bot with moderation, reaction roles, giveaways and more!",
  "main": "dist/index.js",
  "repository": "https://github.com/GoodBoyNeon/byte.git",
  "author": "Sushant Ray <contact@neon.is-a.dev>",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development nodemon src/index.ts",
    "prod": "NODE_ENV=production git pull && yarn dbgen && rm -rf dist/ && yarn build && node .",
    "build": "tsc",
    "start": "yarn prod",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prettier": "prettier . --check",
    "prettier:fix": "prettier . --write",
    "ci": "yarn prettier:fix && yarn lint:fix",
    "dbgen": "prisma format && prisma generate"
  },
  "devDependencies": {
    "@total-typescript/ts-reset": "^0.4.2",
    "@types/get-image-colors": "^4.0.2",
    "@types/ms": "^0.7.31",
    "@types/node": "^20.4.1",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.44.0",
    "nodemon": "^3.0.1",
    "pm2": "^5.3.0",
    "prisma": "^5.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@google-ai/generativelanguage": "^0.2.1",
    "@prisma/client": "5.0.0",
    "@sentry/node": "^7.68.0",
    "@sentry/profiling-node": "^1.2.1",
    "axios": "^1.4.0",
    "console-wizard": "^1.4.2",
    "discord.js": "^14.12.1",
    "dotenv": "^16.3.1",
    "get-image-colors": "^4.0.1",
    "google-auth-library": "^9.0.0",
    "ms": "^2.1.3",
    "prettier": "^3.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
